<?xml version="1.0" encoding="UTF-8" standalone="no" ?><ex4D:plugin xmlns:ex4D="http://www.4d.com/plugin">  <!--SVG Converter-->  <ex4D:documentation name="SVG Converter" plugin-id="20000">    <ex4D:plugin-description>      <!-- write documentation for SVG Converter here -->	  <p>This plugin is a 4D implementation of the <a href="http://live.gnome.org/LibRsvg">rsvg</a> program.</p><p>You can convert SVG to PDF, PNG, PS or SVG.</p>    </ex4D:plugin-description>    <ex4D:commands>      <ex4D:command-group command-group-id="1" name="Convert Many">        <ex4D:command command-id="1" name="SVG Convert array">          <ex4D:command-description>            <!-- write documentation for SVG Convert array here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="images" type="Y">            <!-- write documentation for $1 (ARRAY) here-->			<p>Pass an array of SVG images.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="data" type="O">            <!-- write documentation for $2 (BLOB) here-->			<p>On return, a single document of the images concatenated.</p>          </ex4D:argument>          <ex4D:argument argument-id="3" label="type" type="L">            <!-- write documentation for $3 (LONGINT) here-->			<p>Optional. Target type. Must be either PDF or PS. Default is PDF.</p>          </ex4D:argument>          <ex4D:argument argument-id="4" label="keys" type="Y">            <!-- write documentation for $4 (ARRAY) here-->			<p>Optional. An array of longint key names to configure.</p>          </ex4D:argument>          <ex4D:argument argument-id="5" label="values" type="Y">            <!-- write documentation for $5 (ARRAY) here-->			<p>Optional. An array of real key values to set.</p>          </ex4D:argument>          <ex4D:argument argument-id="6" label="backgroundColor" type="T">            <!-- write documentation for $6 (TEXT) here-->			<p>Optional. Background color in valid CSS notation. black, white, #abccee, #aaa...</p>          </ex4D:argument>          <ex4D:argument argument-id="7" label="baseUri" type="T">            <!-- write documentation for $7 (TEXT) here-->			<p>Optional. Base URI of the SVG.</p>          </ex4D:argument>          <ex4D:return-value label="error" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>On success, 0. -1: svg parse error, -2: image is not svg, -3: invalid export format	.</p>          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="2" name="Convert One">        <ex4D:command command-id="2" name="SVG Convert">          <ex4D:command-description>            <!-- write documentation for SVG Convert here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="image" type="P">            <!-- write documentation for $1 (PICTURE) here-->			<p>Pass an SVG image.</p>          </ex4D:argument>          <ex4D:argument argument-id="2" label="data" type="O">            <!-- write documentation for $2 (BLOB) here-->			<p>On return, the image converted.</p>          </ex4D:argument>          <ex4D:argument argument-id="3" label="type" type="L">            <!-- write documentation for $3 (LONGINT) here-->			<p>Optional. Target type. PNG, SVG, PDF or PS. Default is PDF.</p>          </ex4D:argument>          <ex4D:argument argument-id="4" label="keys" type="Y">            <!-- write documentation for $4 (ARRAY) here-->			<p>Optional. An array of longint key names to configure.</p>          </ex4D:argument>          <ex4D:argument argument-id="5" label="values" type="Y">            <!-- write documentation for $5 (ARRAY) here-->			<p>Optional. An array of real key values to set.</p>          </ex4D:argument>          <ex4D:argument argument-id="6" label="backgroundColor" type="T">            <!-- write documentation for $6 (TEXT) here-->			<p>Optional. Background color in valid CSS notation. black, white, #abccee, #aaa...</p>          </ex4D:argument>          <ex4D:argument argument-id="7" label="baseUri" type="T">            <!-- write documentation for $7 (TEXT) here-->			<p>Optional. Base URI of the SVG.</p>          </ex4D:argument>          <ex4D:return-value label="error" type="L">		  <p>On success, 0. -1: svg parse error, -2: image is not svg, -3: invalid export format	.</p>            <!-- write documentation for return value (LONGINT) here-->          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>    </ex4D:commands>    <ex4D:constants>      <ex4D:constant-group constant-group-id="1" name="SVG Converter Options" number="20000">        <ex4D:constant constant-id="1" name="SVG_DPI_X" type="L" value="0">          <!-- write documentation for SVG_DPI_X (0) here-->        </ex4D:constant>        <ex4D:constant constant-id="2" name="SVG_DPI_Y" type="L" value="1">          <!-- write documentation for SVG_DPI_Y (1) here-->        </ex4D:constant>        <ex4D:constant constant-id="3" name="SVG_ZOOM_X" type="L" value="2">          <!-- write documentation for SVG_ZOOM_X (2) here-->        </ex4D:constant>        <ex4D:constant constant-id="4" name="SVG_ZOOM_Y" type="L" value="3">          <!-- write documentation for SVG_ZOOM_Y (3) here-->        </ex4D:constant>        <ex4D:constant constant-id="5" name="SVG_ZOOM" type="L" value="4">          <!-- write documentation for SVG_ZOOM (4) here-->        </ex4D:constant>        <ex4D:constant constant-id="6" name="SVG_WIDTH" type="L" value="5">          <!-- write documentation for SVG_WIDTH (5) here-->        </ex4D:constant>        <ex4D:constant constant-id="7" name="SVG_HEIGHT" type="L" value="6">          <!-- write documentation for SVG_HEIGHT (6) here-->        </ex4D:constant>        <ex4D:constant constant-id="8" name="SVG_KEEP_ASPECT_RATIO" type="L" value="7">          <!-- write documentation for SVG_KEEP_ASPECT_RATIO (7) here-->        </ex4D:constant>      </ex4D:constant-group>      <ex4D:constant-group constant-group-id="2" name="SVG Output Formats" number="20001">        <ex4D:constant constant-id="1" name="SVG Output PDF" type="L" value="0">          <!-- write documentation for SVG Output PDF (0) here-->        </ex4D:constant>        <ex4D:constant constant-id="2" name="SVG Output PNG" type="L" value="1">          <!-- write documentation for SVG Output PNG (1) here-->        </ex4D:constant>        <ex4D:constant constant-id="3" name="SVG Output PS" type="L" value="2">          <!-- write documentation for SVG Output PS (2) here-->        </ex4D:constant>        <ex4D:constant constant-id="4" name="SVG Output SVG" type="L" value="3">          <!-- write documentation for SVG Output SVG (3) here-->        </ex4D:constant>        <ex4D:constant constant-id="5" name="SVG Output Export" type="L" value="4">          <!-- write documentation for SVG Output Export (4) here-->        </ex4D:constant>      </ex4D:constant-group>    </ex4D:constants>  </ex4D:documentation></ex4D:plugin>